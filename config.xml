<?xml version='1.0' encoding='utf-8'?>
<widget id="com.skillz.examples.cordova2048" version="1.0.0" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <name>Cordova 2048</name>
    <description>
        2048 adapted to Cordova and integrated with the Skillz Cordova crossplatform SDK wrapper.
    </description>
    <author email="integrations@skillz.com" href="http://www.skillz.com">
        Skillz
    </author>
    <content src="index.html" />
    <plugin name="cordova-plugin-whitelist" spec="1" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <platform name="android">
        <allow-intent href="market:*" />
    </platform>
    <platform name="ios">
        <allow-intent href="itms:*" />
        <allow-intent href="itms-apps:*" />
    </platform>
    <preference name="orientation" value="portrait" />
</widget>
